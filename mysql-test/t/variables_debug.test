--source include/have_debug.inc

#
# Bug#34678 @@debug variable's incremental mode
#

set debug= 'T';
select @@debug;
set debug= '+P';
select @@debug;
set debug= '-P';
select @@debug;

#
# Bug#38054: "SET SESSION debug" modifies @@global.debug variable
#

SELECT @@session.debug, @@global.debug;

SET SESSION debug = '';

SELECT @@session.debug, @@global.debug;
