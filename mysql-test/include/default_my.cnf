# Use default setting for mysqld processes
!include default_mysqld.cnf

[mysqld.1]

# Run the master.sh script before starting this process
#!run-master-sh

log-bin=                 master-bin


[mysqlbinlog]
disable-force-if-open

[ENV]
MASTER_MYPORT=           @mysqld.1.port
MASTER_MYSOCK=           @mysqld.1.socket
